{
    "info": {
        "version": "0.1.4",
        "title": "Cool product name"
    },
    "paths": {
        "/Account/login": {
            "post": {
                "tags": ["login"],
                "parameters": [
                    {
                        "name": "client_id",
                        "in": "formData",
                        "required": true
                    },
                    {
                        "name": "client_secret",
                        "in": "formData",
                        "required": true
                    },
                    {
                        "name": "username",
                        "in": "formData",
                        "required": true
                    },
                    {
                        "name": "password",
                        "in": "formData",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "authenticated"
                    },
                    "401": {
                        "description": "authentication failed"
                    }
                }
            }
        },
        "/Account/register": {
            "post": {
                "tags": ["login"],
                "parameters": [
                    {
                        "name": "client_id",
                        "in": "formData",
                        "required": true
                    },
                    {
                        "name": "client_secret",
                        "in": "formData",
                        "required": true
                    },
                    {
                        "name": "Accept: application/json",
                        "in": "header",
                        "required": false
                    },
                    {
                        "name": "username",
                        "in": "formData",
                        "required": true
                    },
                    {
                        "name": "password",
                        "in": "formData",
                        "required": true
                    },
                    {
                        "name": "password_confirm",
                        "in": "formData",
                        "required": true
                    },
                    {
                        "name": "over18",
                        "in": "formData",
                        "required": false
                    },
                    {
                        "name": "email",
                        "in": "formData",
                        "required": false
                    },
                    {
                        "name": "nickname",
                        "in": "formData",
                        "required": false
                    },
                    {
                        "name": "realname",
                        "in": "formData",
                        "required": false
                    },
                    {
                        "name": "career",
                        "in": "formData",
                        "required": false
                    },
                    {
                        "name": "address",
                        "in": "formData",
                        "required": false
                    }
                ],
                 "responses": {
                    "200": {
                        "description": "registered"
                    },
                    "400": {
                        "description": "registered failed"
                    }
                }
            }
        },
        "/Account/logout": {
            "get": {
                "tags": ["login"],
                "parameters": [
                    {
                        "name": "session",
                        "in": "header",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successfully logout"
                    }
                }
            }
        }
    }
}
